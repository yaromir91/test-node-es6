{
  "name": "node-server-es6",
  "version": "1.0.0",
  "description": "Example Node Server w/ Babel",
  "main": "./index.js",
  "scripts": {
    "postinstall": "export APP_ROOT=$pwd/server || set APP_ROOT=$pwd/server",
    "start": "nodemon server/index.js --exec babel-node --presets es2015,stage-0",
    "build": "babel --presets es2015,stage-0 server/ -d build/",
    "serve": "node build/index.js",
    "test": "export NODE_ENV=test || set NODE_ENV=test && ./node_modules/mocha/bin/mocha --compilers js:babel-register ./server/tests/"
  },
  "author": "Yaromir",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.11.5",
    "chai": "^3.5.0",
    "mocha": "^2.3.4",
    "mocha-babel": "^3.0.3",
    "nodemon": "^1.8.1",
    "nyc": "^7.1.0",
    "supertest": "^1.2.0"
  },
  "dependencies": {
    "bluebird": "^3.1.4",
    "body-parser": "^1.14.2",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "express": "4.14.0",
    "express-validation": "1.0.0",
    "express-winston": "^1.2.0",
    "helmet": "2.1.1",
    "http-status": "^0.2.0",
    "joi": "8.4.2",
    "lodash": "^4.0.1",
    "method-override": "^2.3.5",
    "mongoose": "^4.3.7",
    "mongoose-double": "0.0.1",
    "morgan": "1.7.0",
    "supertest-as-promised": "^3.2.0",
    "winston": "^2.1.1"
  }
}
